generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model WaitlistEntry {
  id        String   @id @default(cuid())
  email     String
  name      String?
  note      String?
  source    String?
  createdAt DateTime @default(now())
}
